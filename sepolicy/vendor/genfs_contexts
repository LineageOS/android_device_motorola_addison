# sysfs
genfscon sysfs /devices/soc/6a00000.ssusb/power_supply                u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/7600000.hsusb/power_supply                u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/75b5000.i2c/i2c-7/7-0022/power_supply     u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/msm-bcl-15/power_supply                   u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/soc:qcom,bcl/power_supply                 u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/qpnp-fg-18                                u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/qpnp-smbcharger-17/power_supply           u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/qpnp-smbcharger-17/power_supply/battery/system_temp_level u:object_r:sysfs_battery_supply:s0

genfscon sysfs /devices/soc/soc:qcom,cpubw u:object_r:sysfs_msm_subsys:s0
genfscon sysfs /module/stmvl53l1                                              u:object_r:sysfs_sensor:s0

# Real-time clock
genfscon sysfs /devices/soc/qpnp-rtc-8/rtc                              u:object_r:sysfs_rtc:s0
